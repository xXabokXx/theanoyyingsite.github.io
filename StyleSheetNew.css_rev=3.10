/*** Start left menu styling ***/
#menu_left {
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 12px;
  font-weight: normal;
/*   line-height:14px; Not good for IE*/
/*   height:100%; */
/*     box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05); */
    width:179px;
    float:left;
}
.menu_top, .menu_header, .menu_item, .menu_item_selected{
  border-left: none;
  border-right: 1px solid #cccccc;
  border-bottom: 1px solid transparent;
  border-top: none;
  padding-left: 12px;
  padding-top:6px;
  min-height:16px;
  padding-bottom: 5px;
  vertical-align:middle;
}
.menu_top, .menu_header {
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 12px;
  font-weight: bold;
  color: #343434;
      border-bottom: 1px solid #cccccc;
}
.menu_top {
  padding-top:8px;
      border-right: 1px solid #cccccc;

}
.menu_header {
  height: 20px;
  padding-bottom:1px;
    border-right: 1px solid #cccccc;

}
.menu_item {
  padding-left: 17px;  
  cursor:pointer; cursor:hand;
  border-radius: 0 0 0 0;
  border-bottom: 1px solid #cccccc;
  border-right: 1px solid #cccccc;

  }
.menu_item_selected {      
  padding-left: 17px;
  cursor:pointer; cursor:hand;
  border-radius: 0 0 0 0;
  background:#f5f5f5;
  border-bottom: 1px solid #cccccc;
/*  border-right:none; */
  color:#000000;


}
.menu_item:hover {    
  color: #5f5f5f;
  background:#f5f5f5;
  border-bottom: 1px solid #cccccc;

  padding-left: 17px;  
  cursor:pointer; cursor:hand;
}

/*** End left menu styling ***/

.hand
{
  cursor:pointer; cursor:hand;
}

