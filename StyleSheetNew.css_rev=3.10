  font-weight: normal;
}
img
{
  border: 0;
}
ul
{
  margin: 0;
  padding: 0;
}
li
{
  margin: 0;
  padding: 0;
}

/*-------------------- LOGO COLOR -----------------*/

a
{
  text-decoration:none;
}

/*----------- Elements End ------------*/

/*----------- Various ------------*/
.float-right{
	float: right;
}

.smallBusinessCardPicAdjust{
	width: 50px;
	margin-top: -17px;
	margin-left: 10px;
}

/* Profile picture styles for student */
.profile-picture-border{
  padding:0px;
  border: 2px solid #DDD;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);
}

/* Automatic logout box style student/parent */
#top-box {
  width: 482px;
  height: 0;
  z-index: 1000000;
  background-color: white;
  color: black;
  position: fixed;
  border: 1px solid black;
  display: none;
  padding: 30px;
}
#top-box table{
  width: inherit;
}
#top-box .btn{
  float: inherit;
}

/*----------- Div#Top Start ------------*/
#top
{
  background: url(../images/top.png) no-repeat;
  text-align: left;
  height: 39px;
  min-width: 981px;
  color: #ffffff;
  font-size: 13px;
  font-weight: normal;
  font-style: normal;
  line-height: 1.2;
}
#top_content
{
  padding: 0 0 0 230px;
}
#top_content span.text
{
  vertical-align: top;
}

#top_content span.text span.btn-group{
  float:none;
}
#top_content span.text span.btn-group:before, #top_content span.text span.btn-group:after{
  display:inline;
}

#top_content span.text span.btn-group a.btn{
  float:none;
  display:inline;
}

.divider
{
  position: relative;
  top: -10px;
  margin:0 10px 0 10px;
}
a:active, a:focus {
outline: 0
}
#logout a, #schoolleader a
{
  vertical-align:top;
  color:#ffffff; 
  border-bottom: 1px dotted #ffffff;
}
#logout a:hover, #schoolleader a:hover 
{
  border-bottom: 1px solid #ffffff; 
}
/** wrapper in top menu **/
#top_content_wrapper 
{
  position: absolute;
  left: 206px;
  top: 44px;
  float: left;
  width: 762px;
  padding:4px 4px 0 9px;
}
#content
{
  padding-top:8px;
}

.top-gray-bar-info{
    float: left;
    padding-top: 10px;
}


/*---------- End Div#Top  ------------*/

/*---------- Start Div#menu_left -----*/
